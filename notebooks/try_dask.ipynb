    "%%time\n",
    "import pandas as pd\n",
    "import config\n",
    "\n",
    "\n",
    "q = 'SELECT dr.* ' \\\n",
    "    'FROM dose_response dr ' \\\n",
    "    'INNER JOIN targets tg on dr.AID == tg.PUBCHEM_AID ' \\\n",
    "    'WHERE tg.GeneSymbol == \"ESR1\"'\n",
    "\n",
    "target_data = pd.read_sql_query(q, con=config.Config.DB_URI)\n",
    "\n",
    "print(target_data)"
   ]
